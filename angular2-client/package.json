{
  "name": "angular2-movies",
  "version": "0.0.0",
  "description": "An Angular2 app to see & comment top movies",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf build dist dll",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server --open --progress --profile --watch --content-base src/",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:ngc": "ngc",
    "build": "npm run clean && npm run build:ngc && npm run build:prod",
    "build:server:prod": "npm run build && npm run server:prod",
    "development": "npm run build:dev",
    "lint": "tslint \"src/**/*.ts\"",
    "production": "npm run build:server:prod",
    "server:prod": "http-server dist -c-1 --cors",
    "start": "npm run development",
    "test": "npm run lint && karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run"
  },
  "keywords": [
    "angular2",
    "django"
  ],
  "author": "damnko",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/platform-server": "^4.1.3",
    "@angular/router": "^4.1.3",
    "@ngtools/webpack": "^1.3.1",
    "@types/jasmine": "^2.5.47",
    "@types/lodash": "^4.14.66",
    "@types/node": "^7.0.13",
    "add-asset-html-webpack-plugin": "^2.0.1",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.1.2",
    "codelyzer": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.4.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
